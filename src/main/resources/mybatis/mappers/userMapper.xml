<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lwj.MySpringBoot.login.dao.UserDao">
  
  <!-- 通过用户名查询用户 -->
  <select id="queryUserNumByUserName" parameterType="String" resultType="int">
  	select count(*) from testdb.user where username = #{userName}
  </select>
</mapper>